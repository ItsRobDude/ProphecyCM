{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "character_id": {
      "pattern": "^(?:class|creature|effect|faction|feat|gear|item|level|loc|npc|option|pc|quest|race|save|skill|start)\\.[a-z0-9]+(?:[-_][a-z0-9]+)*$",
      "type": "string"
    },
    "character_type": {
      "type": "string"
    },
    "target_level": {
      "type": "integer"
    }
  },
  "required": [
    "character_id",
    "character_type",
    "target_level"
  ],
  "title": "LevelUpRequest",
  "type": "object"
}
