{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "current_stacks": {
      "type": "integer"
    },
    "dispel_condition": {
      "enum": [
        "any",
        "magic_only",
        "none"
      ],
      "type": "string"
    },
    "duration": {
      "type": "integer"
    },
    "duration_type": {
      "enum": [
        "turns",
        "encounter",
        "rest"
      ],
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "max_stacks": {
      "type": "integer"
    },
    "modifiers": {
      "additionalProperties": {
        "type": "integer"
      },
      "type": "object"
    },
    "name": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "stacking_rule": {
      "enum": [
        "stack",
        "refresh",
        "replace"
      ],
      "type": "string"
    }
  },
  "required": [
    "duration",
    "id",
    "name"
  ],
  "title": "StatusEffect",
  "type": "object"
}
